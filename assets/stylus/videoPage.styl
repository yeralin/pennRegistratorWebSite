@import 'nib'
@import 'requires/*'
abs = absolute

.videoPage .titleLabel
	fontSetFiraSansMedium()
	font-weight 300
	margin-bottom 2%
	color #162952

.midnightHeader.second
	background #F5FFFA
	.left
		padding-left 28%
		margin-left -1.2%
		color #162952
	.right
		width 50%
		color #162952

.videoPage #first
	position abs
	width 100%
	height 100%
	z-index -1 !important
	opacity 1
	background-color #F5FFFA
	background-repeat no-repeat
	background-size 100% 100%